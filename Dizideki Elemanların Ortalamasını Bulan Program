import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        double[] list ={1,2,3,4,5};
        double sum=0.0;
        for (int i=0;i<list.length;i++){
            sum+=1/list[i];

        }
        double avarage=sum/list.length;
        System.out.println(avarage);
    }
}
