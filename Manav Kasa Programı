import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);

        double armut=2.14,elma=3.67, domates=1.11, muz= 0.95, patlican=5.00,toptutar;
        int armutAdet,elmaAdet,domatesAdet,muzAdet,patlicanAdet;

        System.out.print("Armut Kaç Kilo ? :");
        armutAdet=input.nextInt();
        System.out.print("Elma Kaç Kilo ? :");
        elmaAdet=input.nextInt();
        System.out.print("Domates Kaç Kilo ? :");
        domatesAdet=input.nextInt();
        System.out.print("Muz Kaç Kilo ? :");
        muzAdet=input.nextInt();
        System.out.print("Patlıcan Kaç Kilo ? :");
        patlicanAdet=input.nextInt();

        toptutar=(armut*armutAdet)+(elma*elmaAdet)+(domates*domatesAdet)+(muz*muzAdet)+(patlican*patlicanAdet);
        System.out.println("Toplam tutar: "+toptutar);
    }
}
