import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

       int n;
       double sonuc=0;
        System.out.print("N sayısı gir:");
        n=input.nextInt();

        for (int i=1;i<=n;i++)
            sonuc=sonuc+(1.0/i);

        System.out.print("Harmonik sayı: "+sonuc);
    }
}
